    function toggleDiv(divNumber) {
        const divs = document.querySelectorAll('.toggleDiv');
        const activeDiv = document.getElementById('div' + divNumber);

        divs.forEach((div) => {
            const divId = div.getAttribute('onclick').match(/\d+/)[0];
            const targetDiv = document.getElementById('div' + divId);

            if (divId === divNumber.toString()) {

                if (targetDiv.classList.contains('hidden')) {
                    targetDiv.classList.remove('hidden');
                }
            } else {

                if (!targetDiv.classList.contains('hidden')) {
                    targetDiv.classList.add('hidden');
                }
            }
        });
