        function addMessage(sender, text) {
            var timestamp = new Date().toLocaleTimeString();
            var messageClass = sender === 'user' ? 'user-message' : 'bot-message';
            var messageDiv = document.createElement('div');
            messageDiv.classList.add('message', messageClass);
            messageDiv.innerHTML = `
                <span class="timestamp">${timestamp}</span>
                <p>${text}</p>
            `;
            document.querySelector('.chat-container').appendChild(messageDiv);
        }

        document.getElementById('chat-send-button').addEventListener('click', function () {
            var userMessageInput = document.getElementById('chat-user-input');
            var userMessage = userMessageInput.value;
            if (userMessage.trim() !== '') {
                addMessage('user', userMessage);

                setTimeout(function () {
                    let botResponse = "I'm sorry, I didn't understand that.";
                    for (const entry of messageResponses) {
                        if (entry.userMessage.includes(userMessage.trim().toLowerCase())) {
                            botResponse = entry.botResponse;
                            break;
                        }
                    }

                    addMessage('bot', botResponse);



                    if (botResponse === "I'm sorry, I didn't understand that.") {
                        const lastUserMessage = getLastUserMessage();
                        const contextSuggestions = getContextAwareSuggestions(lastUserMessage);

                        if (contextSuggestions.length > 0) {
                            setTimeout(function () {
                                addMessage('bot', "I'm not sure I understand. Did you mean:");
                                for (const suggestion of contextSuggestions) {
                                    setTimeout(function () {
                                        addMessage('bot', suggestion);
                                    }, 500);
                                }
                            }, 1000);
                        } else {
                            setTimeout(function () {
                                addMessage('bot', "I'm not sure I understand. Could you please provide more information?");
                            }, 1000);
                        }
                    }
                }, 1000);

                userMessageInput.value = '';
            }
        });


        function getLastUserMessage() {
           
            const messageElements = Array.from(document.getElementsByClassName('user-message'))[0].getElementsByTagName('p');

            if (messageElements.length > 0) {
                return messageElements[messageElements.length - 1].textContent;
            }

            return '';
        }


        function getContextAwareSuggestions(userMessage, conversationHistory) {
            const keywordSuggestions = {
                "clarify": "Could you clarify what you meant by that?",
                "assist": "I'm here to assist you! How about asking me a question on a specific topic?",
                "explore": "Feel free to explore different topics. You can ask about science, history, technology, and more!",
            };

            for (const keyword in keywordSuggestions) {
                if (userMessage.toLowerCase().includes(keyword)) {
                    return [keywordSuggestions[keyword]];
                }
            }
            return ["I'm here to help! How can I assist you today?"];
        }


