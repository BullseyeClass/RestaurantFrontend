        new  RestaurantFrontend.Models.CustomerSupportEntity.CustomerSupport { FirstName = "user", Message = "Hello! How can I help you?", Timestamp = DateTime.Now },
        new RestaurantFrontend.Models.CustomerSupportEntity.CustomerSupport { FirstName = "bot", Message = "Hi there! I'm here to assist you.", Timestamp = DateTime.Now.AddMinutes(1) },
    };
}




