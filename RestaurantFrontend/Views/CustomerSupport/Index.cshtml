<div>
    <div class="offcanvas offcanvas-bottom-end offcanvas-end " data-bs-backdrop="false" data-bs-scroll="true" style="  box-shadow: -8px 8px 32px -1px rgba(0,0,0,0.23); border-bottom-left-radius:1rem; max-width:21.5rem; border-bottom-right-radius:1rem; margin-right:1.3rem; margin-bottom:1.3rem;" tabindex="-1" id="offcanvasChat" aria-labelledby="offcanvasRightLabel">
        <div class="offcanvas-header" style="background-color:#8c0303; id="cart-modal">
            <div>

            <h5 style="color:white; margin-bottom:0; font-weight:600;">BullsMart.</h5>
                <p style="color:white; font-size:13px; margin-top:0px;"> <span style=" min-width:10px; border-radius:25px; min-height:10px;display:inline-flex; background-color:#4ab12a;"></span> We’ll reply as soon as we can</p>

            </div>
            <button type="button" style="color:white;" class="btn-close btn-light btn-close-custom" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div class="offcanvas-body mt-5 cart-body">


            <div class="chat-container">
                @foreach (var message in chatMessages)
                {
                    <div class="message  @(message.FirstName)-message">
                        <span class="timestamp">@message.Timestamp.ToString("HH:mm")</span>
                        <p>@message.Message</p>
                    </div>
                }
               
            </div>

            <div class="chat-input-container">
                <input type="text" id="chat-user-input" placeholder="Type your message..." />
                <button id="chat-send-button">Send</button>
            </div>
        </div>
    </div>
</div>

<h1 id="support-section">Customer Support</h1>
<div class="top-Paragraph-Container">
    <p>Welcome to our help center. Click for personalized support and updates. 
        Your destination for solutions and connection.</p>
</div>

<div class="main-form-container">
<div class="form-container">
    <img src="~/images/CustomerServiceImage.jpg" />

        <div>

            <div class=" form-contents">
                <h2>We're Here to Help!</h2>
                <p>Fill out the form with any query on  <br />your mind and we'll get back to you as  <br />soon as possible</p>
                <div class="form-env">
                    <form asp-action="Index">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="input-group1">
                            <div class="form-group">
                                <label asp-for="FirstName" class="form-label">First Name</label>
                                <input asp-for="FirstName" class="input-box" />
                                <span asp-validation-for="FirstName" class="text-danger form-validation"></span>
                            </div>
                            <div class="input-container">
                                <label asp-for="LastName" class="form-label">Last Name</label>
                                <input asp-for="LastName" class="input-box" />
                                <span asp-validation-for="LastName" class="text-danger form-validation"></span>
                            </div>
                        </div>

                        <div class="input-group1">
                            <div class="form-group">
                                <label asp-for="Email" class="form-label"> Email *</label>
                                <input asp-for="Email" class="input-box" />
                                <span asp-validation-for="Email" class="text-danger form-validation"></span>
                            </div>
                            <div class="input-container">
                                <label asp-for="PhoneNumber" class="form-label">Phone</label>
                                <input asp-for="PhoneNumber" class="input-box" />
                                <span asp-validation-for="PhoneNumber" class="text-danger form-validation"></span>
                            </div>
                        </div>

                        <div class="input-group1">
                            <div class="form-group">
                                <label asp-for="Message" class="form-label">Leave us a message...</label>
                                <textarea asp-for="Message" class="input-message " rows="1" placeholder=""></textarea>

                            </div>
                            <span asp-validation-for="Message" class="text-danger form-validation"></span>

                        </div>
                        <div class="form-group">
                            <a type="submit" value="Create" class="submit-button"> Submit </a>
                        </div>

                    </form>
                </div>

                <div class="Chat-with-us">

                   <h4>Chat with Our Support Team</h4>
                   <p>Connect with us. Click here to initiate a conversation and <br />
                       customize your experience with our dedicated support.</p>
                    <div class="form-group">
                        <a data-bs-toggle="offcanvas" data-bs-target="#offcanvasChat" aria-controls="offcanvasChat" class="message-button"> Message Us </a>
                    </div>
                </div>
            </div>

        </div>

    </div>
</div>





